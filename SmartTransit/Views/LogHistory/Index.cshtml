@model IEnumerable<SmartTransit.Models.Delivery>

@{
    ViewBag.Title = "Log History";
}

<h3>Log History</h3>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tracking ID
        </th>
        <th>
            Date
        </th>
        <th>
            Log History
        </th>
        >
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryID)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.DeliveryID })


            </td>
            <td>


        </tr>
    }

</table>
